<mjml>
  <mj-head>
    <mj-attributes>
      <mj-text font-family="Arial" align="center" padding="0" color="#1d1d1f" />
    </mj-attributes>
  </mj-head>
  <mj-body background-color="#f6f4ff">
    <mj-section text-align="center">
      <mj-column padding="0 10px">
        <mj-image src="../assets/images/icons/flatquest.png" alt="FlatQuest" width="150px" />
        <mj-spacer height="40px" />

        <mj-text font-size="24px" font-weight="700" color="rgba(57, 62, 64, 1)" align="center">
